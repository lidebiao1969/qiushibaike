.comment-ding{
	margin-left: 5rpx;
}
.comment-share{
	margin-right: 5rpx;
}
.index-list3{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.index-video{
	position: relative;
	width: 100%;
	height: 300rpx;
}
.index-play{
	position: absolute;
	font-size: 30px;
	right: 50%;
	top:50%
}
.index-message{
	position: absolute;
	font-size: 30rpx;
	bottom: 5px;
	right: 10px;
	color: red;
	background-color: #0077AA;
	border-radius: 10px;
	z-index: 3;
}

